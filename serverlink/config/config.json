{
  "server_port": 8080,
  "ws_port": 8081,
  "ws_connect_timeout": 5000,
  "ws_socket_timeout": 30000,
  "graph_version": "serverlink@1.0.0",
  "endpoint_host": "http://192.168.1.234:8001",
  "endpoints" : {
    
    "get_datasets": {
      "dest": "$HOST",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      },
      "queries":["from","to","datumId","projectId","name"]
    },
    "create_dataset_from_labels":{
      "dest":"$HOST/dataset",
      "method":"post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body":{"type":"raw"}
    },
    "create_dataset_from_labels_and_data":{
      "dest":"$HOST/dataset/upload",
      "method":"post",
      "headers": {
        "Content-Type": "multipart/form-data"
      },
      "body":{
        "type":"form-data",
        "fields":[
          {"name":"name"},
          {"name":"projectId"},
          {"name":"label"},
          {"name":"data"}
        ]
      }
    },
    "get_dataset_by_id": {
      "dest": "$HOST/dataset/{datasetId}",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      }
    },

    "get_images_in_project":{
      "dest":"$HOST/label/session/image",
      "method": "get",
      "headers":{
        "Accept": "application/json"
      },
      "queries":["projectId"]
    },
    "upload label data during a labeling session":{
      "dest": "$HOST/label/session/image",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body":{"type":"raw"}
    },
    
    "create_label_session_from_proj_id": {
      "dest": "$HOST/label/session/create",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {"type": "raw"}
    },
    "persist_label_session_from_proj_id": {
      "dest": "$HOST/label/session/persist",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {"type": "raw"}
    },
    "get_label_session_status": {
      "dest": "$HOST/label/session/status",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      },
      "queries":["projectId"]
    },
    
    "get_training_labels": {
      "dest": "$HOST/label",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      },
      "queries":["from","to","datumId","projectId","datasetId"]
    },
    "add_label": {
      "dest": "$HOST/label",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "type": "raw"
      }
    },

    "run_inference_with_model":{
      "dest": "$HOST/model/inference",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      },
      "queries":["datumId","modelId"]
    },
    "init_inference_model":{
      "dest": "$HOST/model/inference/init",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      },
      "queries":["datumId","modelId"]
    },
    "debug_run_inference_with_file":{
      "dest": "$HOST/model/inference/raw",
      "method": "post",
      "headers": {
        "Content-Type": "multipart/form-data",
        "Accept":"application/json"
      },
      "body":{
        "type":"form-data",
        "fields":[
          {"name":"datumFile","type":"file"},
          {"name":"modelId"}
        ]
      }
    },

    "query_paginated_projects":{
      "dest": "$HOST/project",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      },
      "queries":["from","to","datumId","datasetId","name"]
    },
    "create_new_proj":{
      "dest": "$HOST/project",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body":{
        "type":"raw"
      }
    },
    "get_proj_by_id":{
      "dest": "$HOST/project/{projectId}",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      }
    },

    "add_artifact_during_train_session":{
      "dest": "$HOST/train/artifact",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body":{
        "type":"form-data",
        "fields":[{"name":"weightsFile","type":"file"}]
      },
      "queries":["modelId","datasetId"]
    },
    "run_train_start":{
      "dest": "$HOST/train/start",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body":{
        "type":"raw"
      }
    },
    "get_train_status":{
      "dest": "$HOST/train/status",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      }
    },
    "run_train_stop":{
      "dest": "$HOST/train/stop",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body":{
        "type":"raw"
      }
    },

    "query_paginated_models":{
      "dest": "$HOST/trained/model",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      },
      "queries":["from","to","projectId","name"]
    },
    "add_trained_model":{
      "dest": "$HOST/trained/model",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "body":{
        "type":"form-data",
        "fields":[
          {"name":"modelTemplateId"},
          {"name":"projectId"},
          {"name":"classes"},
          {"name":"modelName"},
          {"name":"modelTemplateFileParams"}
        ]
      }
    },

    "get_all_mothership_classes":{
      "dest": "$HOST/classes",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      }
    },
    "get_all_supported_models":{
      "dest": "$HOST/models",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      }
    },
    "query_server_templates":{
      "dest": "$HOST/template",
      "method": "get",
      "headers": {
        "Accept": "application/json"
      },
      "queries":["from","to"]
    }
  }
}